pipeline
{
 agent any 
 stages{
    stage('Build Application'){
    steps{
    bat 'mvn clean install -DskipTests'
    }
    }
  
   stage('Ok') {
   steps {
     echo "Ok"
   }
   }
   
   stage('email notification'){
   steps {
   emailext attachLog: true, body: 'email---', subject: 'from jenkins', to: 'sravanidevarapalli17@gmail.com'
   
  }
  }
  }
  
}
